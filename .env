# Myanmar Election Visualization - Environment Configuration
# Copy this file to .env and add your API keys

# Optional: Custom map tile server
# CUSTOM_MAP_SERVER_URL=https://your-custom-tiles.server.com/{z}/{x}/{y}.png

# Application Settings
DEBUG_MODE=false
ENABLE_PERFORMANCE_MONITORING=true

# Map Configuration
DEFAULT_ZOOM_LEVEL=6
DEFAULT_MAP_PROVIDER=auto
HEAT_MAP_RADIUS=20
HEAT_MAP_BLUR=10
HEAT_MAP_MIN_OPACITY=0.15
HEAT_MAP_INTENSITY_SCALE=0.8

# Cache Settings  
ENABLE_CACHING=true
CACHE_TTL_SECONDS=3600
